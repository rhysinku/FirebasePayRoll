<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FirebasePayRoll.Delete">
    <ContentPage.Content>
        <StackLayout>
            <Entry x:Name="empnum" Placeholder="Employee Number" />

            <StackLayout>
                <Button Text="Delete" Clicked="btnDelete"/>
                <Button Text="Home Page" Clicked="btnHome"/>
            </StackLayout>
        </StackLayout>
        
    </ContentPage.Content>
</ContentPage>